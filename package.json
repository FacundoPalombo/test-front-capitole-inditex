    "jest-fetch-mock": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.13.0",
    "react-spinners": "^0.13.8",
    "remark-gfm": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write",
