  "description": "Prueba tecnica de capitole para el cliente inditex, hecho por Facundo Palombo",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "prod": "concurrently \"npm:build\" \"npm:serve:prod\"",
