  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "dev:node": "concurrently \"npm:watch\" \"npm:serve:node\"",
    "serve:node": "node ./bin/www",
    "prod": "concurrently \"npm:build\" \"npm:start\"",
