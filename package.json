    "test": "jest --config=jest.config.js",
    "build": "webpack --mode=production --node-env=production",
    "build:server": "tsc -b server/tsconfig.json",
    "build:dev": "webpack --mode=development",
    "watch:ui": "webpack --watch --mode=development",
    "watch:server": "tsc -b server/tsconfig.json --watch",
    "watch": "concurrently \"npm:watch:ui\" \"npm:watch:server\" \"npm:start\"",
    "webpack:serve": "webpack serve",
    "start": "node ./bin/www",
    "prepare": "husky install",
    "lint": "eslint --fix **/**/*.js",
    "analyze": "webpack --mode=production --profile --json > stats.json"
